{
  "expo": {
    "name": "GKP Radio",
    "slug": "gkp-radio",
    "version": "1.0.0",
    "description": "God Kingdom Principles Radio - Broadcasting Truth, Building Community, Transforming Lives",
    "orientation": "portrait",
    "icon": "./mobile/assets/icon.png",
    "userInterfaceStyle": "light",
    "owner": "gkp-radio",
    "scheme": "gkpradio",
    "primaryColor": "#047857",
    "splash": {
      "image": "./mobile/assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gkpradio.mobile",
      "buildNumber": "1.0.1",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSCameraUsageDescription": "GKP Radio needs access to your camera to share moments from our community.",
